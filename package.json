{
  "name": "certik-swc-registry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "update-swc": "cd scripts && npm install && npm run update-swc && cd ..",
    "predev": "yarn update-swc",
    "dev": "next dev",
    "prebuild": "yarn update-swc",
    "build": "next build",
    "postbuild": "next-on-netlify",
    "format": "prettier --write 'components/**/*' 'pages/**/*' '!**/*/.DS_Store'",
    "start": "next start"
  },
  "dependencies": {
    "@ant-design/icons": "^4.3.0",
    "antd": "^4.9.4",
    "autoprefixer": "^10.0.4",
    "next": "10.0.3",
    "next-on-netlify": "^2.6.3",
    "postcss": "^8.1.10",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-error-boundary": "^3.1.0",
    "react-hubspot-form": "^1.3.7",
    "react-i18next": "^11.8.4",
    "react-markdown": "^5.0.3",
    "react-responsive-picture": "^3.2.2",
    "react-syntax-highlighter": "^15.3.1",
    "tailwindcss": "^2.0.1"
  },
  "devDependencies": {
    "prettier": "^2.2.1"
  }
}
